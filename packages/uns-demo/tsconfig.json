{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "es2022",
    "moduleResolution": "bundler",
    "strict": false,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strictPropertyInitialization": false,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "types": [
      "node"
    ],
    "moduleDetection": "force"
  },
  "include": [
    "src/**/*.ts"
  ]
}
