{
  "name": "@uns-kit/cli",
  "version": "0.0.8",
  "description": "Command line scaffolding tool for UNS applications",
  "type": "module",
  "license": "MIT",
  "author": "Aljo\u0161a Vister <aljosa.vister@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/uns-datahub/uns-kit.git",
    "directory": "packages/uns-cli"
  },
  "bin": {
    "uns-kit": "./dist/index.js"
  },
  "keywords": [
    "uns",
    "cli",
    "scaffold",
    "template",
    "typescript"
  ],
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "publish": "pnpm publish --filter @uns-kit/cli --access public"
  },
  "dependencies": {
    "@uns-kit/core": "workspace:*"
  }
}
