  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "demo: launch",
      "cwd": "${workspaceFolder}/packages/uns-demo",
      "runtimeArgs": [
        "--enable-source-maps",
        "--import",
        "tsx"
      ],
      "program": "${workspaceFolder}/packages/uns-demo/src/index.ts",
      "autoAttachChildProcesses": true,
      "outputCapture": "std",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**"
      ],
      "sourceMapPathOverrides": {
        "${workspaceFolder}/**/node_modules/@uns-kit/*/src/*": "${workspaceFolder}/packages/uns-*/src/*"
      },
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "prep: core dist",
      "outFiles": [
        "${workspaceFolder}/packages/uns-core/dist/**/*.js",
        "${workspaceFolder}/packages/uns-demo/dist/**/*.js"
      ]
    },
